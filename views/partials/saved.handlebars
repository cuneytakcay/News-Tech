<!-- SAVED ARTICLES ARE LISTED HERE -->
{{#each articles}}
<div class="col-10">
  <a href="{{ this.link }}" target="_blank">{{ this.title }}</a>
</div>
<div class="col-2">
  <button data-id="{{ this.id }}" data-title="{{ this.title }}" data-link="{{ this.link }}" class="btn btn-sm btn-success float-right mb-1 pl-3 pr-3 add-note">&nbsp;Add Note&nbsp;</button>
  
  <button data-id="{{ this.id }}" data-title="{{ this.title }}" data-link="{{ this.link }}" class="btn btn-sm btn-danger float-right delete-btn">Delete Article</button>
</div>
{{/each}}